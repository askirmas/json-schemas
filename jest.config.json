{
  "preset": "ts-jest",
  "testEnvironment": "node",
  "collectCoverage": true,
  "collectCoverageFrom": [
    "*.[tj]sx?"
  ],
  "coverageThreshold": {
    "global": {
      "branches": 100,
      "functions": 100,
      "lines": 100,
      "statements": 100
    }
  },
  "roots": [
    "utils"
  ]
}